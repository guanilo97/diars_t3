


<section class="content">
    <a href="/home/create" class="btn btn-primary" style="margin-left:80%">Crear Rutina</a>
    <br />
    <br />
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Rutina</th>
                <th>Opciones</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Nombre_Rutina</td>
                    <td><a href="/Home/Ejercicios/?id=@item.Id" class="btn btn-success">Ejercicios</a></td>
                </tr>
            }
        </tbody>
    </table>

</section>
